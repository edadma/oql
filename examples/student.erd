entity class {
 *id: integer
  name: text
  students: [student] (enrollment)
}

entity student {
 *id: integer
  name: text
  classes: [class] (enrollment)
  enrollment: [enrollment]
}

entity enrollment (student_class) {
  student (studentid): student
  class (classid): class
  year: integer
  semester: text
  grade: text
}