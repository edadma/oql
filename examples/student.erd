entity class {
 *id: integer
  name: text
  students: [student] (enrollment)
}

entity laptop {
 *id: integer
  make: text
  model: text
}

entity student {
 *id: integer
  name: text
  laptop: laptop
  classes: [class] (enrollment)
}

entity enrollment {
  student (studentid): student
  class (classid): class
}